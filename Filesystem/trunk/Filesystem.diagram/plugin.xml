   <extension point="org.eclipse.ui.navigator.navigatorContent" id="navigator-content">
      <?gmfgen generated="true"?>
      <navigatorContent
            id="Filesystem.diagram.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="jfb.examples.gmf.filesystem.diagram.navigator.FilesystemNavigatorContentProvider" 
            labelProvider="jfb.examples.gmf.filesystem.diagram.navigator.FilesystemNavigatorLabelProvider"
            icon="icons/obj16/FilesystemDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="filesystem_diagram"/>
               </and>
               <instanceof value="jfb.examples.gmf.filesystem.diagram.navigator.FilesystemAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="jfb.examples.gmf.filesystem.diagram.navigator.FilesystemAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="Filesystem.diagram.navigatorSorter" 
               class="jfb.examples.gmf.filesystem.diagram.navigator.FilesystemNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="filesystem_diagram"/>
                  </and>
                  <instanceof value="jfb.examples.gmf.filesystem.diagram.navigator.FilesystemAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            id="Filesystem.diagram.domainResourceContent" 
            name="%domainNavigatorContentName" 
            priority="normal" 
            contentProvider="jfb.examples.gmf.filesystem.diagram.navigator.FilesystemDomainNavigatorContentProvider" 
            labelProvider="jfb.examples.gmf.filesystem.diagram.navigator.FilesystemDomainNavigatorLabelProvider"
            icon="icons/obj16/FilesystemDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="filesystem"/>
               </and>
               <instanceof value="jfb.examples.gmf.filesystem.diagram.navigator.FilesystemDomainNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof value="jfb.examples.gmf.filesystem.diagram.navigator.FilesystemDomainNavigatorItem"/>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            id="Filesystem.diagram.navigatorActionProvider"
            class="jfb.examples.gmf.filesystem.diagram.navigator.FilesystemNavigatorActionProvider">
         <enablement>
            <or>
               <instanceof value="jfb.examples.gmf.filesystem.diagram.navigator.FilesystemAbstractNavigatorItem"/>
            </or>
         </enablement>
      </actionProvider>
   </extension>
   
